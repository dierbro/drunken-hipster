{
	"config_version": "2",
	"platform_version": "v1.3",
	"name": "hn-mobile",
	"author": "dierbro@gmail.com",
	"version": "0.1",
	"description": "My Extension Description",
	"modules": {
		"activations": [
			{
				"patterns": ["http://*/*", "https://*/*"],
				"styles": [],
				"scripts": ["js/every-page.js"],
				"run_at": "start",
				"all_frames": false
			}
		],
		"background": {
			"files": ["js/background.js"]
		},
		"button": {
			"default_title": "An example browser toolbar icon",
			"default_popup": "index.html",
			"default_icon": "img/logo-forge.png"
		},
		"contact": true,
		"event": true,
		"file": true,
		"geolocation": true,
		"is": true,
		"logging": {
			"level": "INFO"
		},
		"message": true,
		"notification": true,
		"prefs": true,
		"request": {
			"permissions": []
		},
		"sms": true,
		"tabs": true,
		"tools": true,
		"topbar": true,
		"icons": {
            "android": {
                "36": "img/icons/android/36.png",
                "48": "img/icons/android/48.png",
                "72": "img/icons/android/72.png"
            },
            "ios": {
                "57": "img/icons/iOS/57.png",
                "72": "img/icons/iOS/72.png",
                "114": "img/icons/iOS/114.png",
                "prerendered": true
            }
        }
	}
}
